    </servlet>

    <servlet-mapping>
        <servlet-name>RichestUser</servlet-name>
        <url-pattern>/RichestUser</url-pattern>
    </servlet-mapping>
    <!-- ListServlet -->
    <servlet>
        <servlet-name>ListServlet</servlet-name>
        <servlet-class>Controller.Servlets.ListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ListServlet</servlet-name>
        <url-pattern>/ListServlet</url-pattern>
